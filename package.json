{
  "name": "mfa-nodejs",
  "version": "1.0.0",
  "description": "mfa with nodejs",
  "main": "server.js",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "start": "npm run seed:db && npm run start-db & node src/server",
    "dev": "npm run seed:db && npm run start-db & npx nodemon src/server",
    "seed:db": "node database/seed",
    "start:db": "json-server --watch database/db.json",
    "commit": "npx cz",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willaug/mfa-nodejs.git"
  },
  "keywords": [
    "mfa",
    "nodejs",
    "mfa-nodejs",
    "mfa-node",
    "2fa",
    "2fa-node"
  ],
  "author": "William Augusto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/willaug/mfa-nodejs/issues"
  },
  "homepage": "https://github.com/willaug/mfa-nodejs#readme",
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@graphql-tools/merge": "^8.2.2",
    "apollo-server": "^3.6.3",
    "axios": "^0.26.0",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "json-server": "^0.17.0",
    "otplib": "^12.0.1"
  }
}
